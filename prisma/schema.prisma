generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Feedback {
    id        Int      @id @default(autoincrement())
    name      String
    email     String
    company   String?
    features  String[]
    comments  String?  @db.Text
    createdAt DateTime @default(now())
}
